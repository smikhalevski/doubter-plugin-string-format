{
  "name": "@doubter/plugin-string-format",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "disableSources": true,
  "hideGenerator": true,
  "readme": "none",
  "tsconfig": "./tsconfig.json",
  "customCss": "./node_modules/typedoc-custom-css/custom.css",
  "entryPoints": [
    "./src/main/index.typedoc.ts"
  ],
  "externalSymbolLinkMappings": {
    "doubter": {
      "StringShape": "https://smikhalevski.github.io/doubter/latest/classes/core.StringShape.html",
      "IssueOptions": "https://smikhalevski.github.io/doubter/latest/interfaces/core.IssueOptions.html",
      "Message": "https://smikhalevski.github.io/doubter/latest/types/core.Message.html"
    },
    "doubter/core": {
      "Issue.message": "https://smikhalevski.github.io/doubter/latest/interfaces/core.Issue.html#message",
      "Issue.meta": "https://smikhalevski.github.io/doubter/latest/interfaces/core.Issue.html#meta"
    }
  }
}
