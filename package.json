{
  "name": "@doubter/plugin-string-format",
  "version": "1.0.1",
  "description": "String format validation plugin for Doubter.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./ascii": {
      "types": "./ascii.d.ts",
      "import": "./ascii.mjs",
      "require": "./ascii.js"
    },
    "./bic": {
      "types": "./bic.d.ts",
      "import": "./bic.mjs",
      "require": "./bic.js"
    },
    "./email": {
      "types": "./email.d.ts",
      "import": "./email.mjs",
      "require": "./email.js"
    },
    "./fqdn": {
      "types": "./fqdn.d.ts",
      "import": "./fqdn.mjs",
      "require": "./fqdn.js"
    },
    "./imei": {
      "types": "./imei.d.ts",
      "import": "./imei.mjs",
      "require": "./imei.js"
    },
    "./ip": {
      "types": "./ip.d.ts",
      "import": "./ip.mjs",
      "require": "./ip.js"
    },
    "./isin": {
      "types": "./isin.d.ts",
      "import": "./isin.mjs",
      "require": "./isin.js"
    },
    "./luhn": {
      "types": "./luhn.d.ts",
      "import": "./luhn.mjs",
      "require": "./luhn.js"
    },
    "./mime": {
      "types": "./mime.d.ts",
      "import": "./mime.mjs",
      "require": "./mime.js"
    },
    "./uuid": {
      "types": "./uuid.d.ts",
      "import": "./uuid.mjs",
      "require": "./uuid.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf lib docs coverage",
    "test": "jest",
    "perf": "toofast",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/doubter-plugin-string-format.git"
  },
  "keywords": [
    "doubter",
    "plugin",
    "string",
    "format",
    "bic",
    "email",
    "fqdn",
    "imei",
    "ip",
    "isin",
    "luhn",
    "mime",
    "uuid"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/doubter-plugin-string-format/issues"
  },
  "homepage": "https://github.com/smikhalevski/doubter-plugin-string-format#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.10",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.3",
    "typedoc-custom-css": "github:smikhalevski/typedoc-custom-css#master",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "doubter": "^4.0.0",
    "validator": "^13.11.0"
  }
}
