{
  "name": "@doubter/plugin-string-format",
  "version": "1.0.0",
  "description": "String format validation plugin for Doubter.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./bic": {
      "types": "./bic.d.ts",
      "import": "./bic.mjs",
      "require": "./bic.js"
    },
    "./email": {
      "types": "./email.d.ts",
      "import": "./email.mjs",
      "require": "./email.js"
    },
    "./fqdn": {
      "types": "./fqdn.d.ts",
      "import": "./fqdn.mjs",
      "require": "./fqdn.js"
    },
    "./imei": {
      "types": "./imei.d.ts",
      "import": "./imei.mjs",
      "require": "./imei.js"
    },
    "./isin": {
      "types": "./isin.d.ts",
      "import": "./isin.mjs",
      "require": "./isin.js"
    },
    "./luhn": {
      "types": "./luhn.d.ts",
      "import": "./luhn.mjs",
      "require": "./luhn.js"
    },
    "./mime": {
      "types": "./mime.d.ts",
      "import": "./mime.mjs",
      "require": "./mime.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json && rollup --config && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf gen lib docs coverage",
    "test": "jest",
    "docs": "typedoc src/main/index.typedoc.ts src/main/luhn.ts src/main/mime.ts src/main/imei.ts src/main/email.ts src/main/fqdn.ts src/main/isin.ts src/main/bic.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/doubter-plugin-string-format.git"
  },
  "keywords": [
    "doubter",
    "plugin",
    "string",
    "bic",
    "email",
    "fqdn",
    "imei",
    "isin",
    "luhn",
    "mime"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/doubter-plugin-string-format/issues"
  },
  "homepage": "https://github.com/smikhalevski/doubter-plugin-string-format#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.0",
    "rollup-plugin-dts": "^5.3.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.1",
    "typedoc": "^0.24.8",
    "typedoc-custom-css": "github:smikhalevski/typedoc-custom-css#master",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "doubter": "^3.0.0",
    "validator": "^13.9.0"
  }
}
